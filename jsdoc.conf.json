{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./src"]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts" : {
    "destination": "./docs",
    "recurse": true,
    "encoding": "utf8",
    "verbose": true
  },
  "docdash": {
    "search": true,
    "typedefs": true
  }
}