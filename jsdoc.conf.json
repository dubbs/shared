{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./src"],
    "exclude": ["./node_modules", "./test"]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts" : {
    "template": "node_modules/docdash",
    "destination": "./docs",
    "recurse": true
  }
}